smallest=None
largest=None
while True:
    ip=input("enter: ")
    if ip=="done":
        break
    try:
        ip=int(ip)
        if  smallest is None:
            smallest=ip
        if largest is None:
            largest=ip
        elif  ip<smallest:
              smallest=ip
        elif  ip>largest:
            largest=ip
    except:
        print("Invalid input")
print("Maximum:",largest)
print("Minimum",smallest)
